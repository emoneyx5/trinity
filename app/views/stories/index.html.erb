<h1> all the stories </h1>


<% @stories.each do |story| %>
	<div class="story_box">
		<%= link_to story.title, story %><br>
		<%= story.body %><br>
		<% unless story.keyword == nil %>
			<%= story.keyword.wordone %>,
			<%= story.keyword.wordtwo %>,
			<%= story.keyword.wordthree %>
		<% end %>

	</div>
<% end %>